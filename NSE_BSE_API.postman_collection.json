            "body": {
              "mode": "raw",
              "raw": "{\n  \"exchanges\": [\"NSE_EQ\", \"BSE_EQ\"],\n  \"start_date\": \"2025-10-01\",\n  \"end_date\": \"2025-11-02\"\n}"
