            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{}"
            },
            "url": {
              "raw": "{{baseUrl}}/refresh/NSE_EQ",
              "host": ["{{baseUrl}}"],
              "path": ["refresh", "NSE_EQ"]
            },
            "description": "Refresh NSE Equity data with automatic date calculation"
          }
        },
        {
          "name": "Refresh NSE_EQ (Custom Dates)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"start_date\": \"2025-10-01\",\n  \"end_date\": \"2025-11-02\"\n}"
